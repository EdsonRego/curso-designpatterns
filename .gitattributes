# Maven Wrapper scripts
mvnw text eol=lf
mvnw.cmd text eol=crlf

# Arquivos de configuração comuns
*.properties text eol=lf
*.xml text eol=lf
*.yml text eol=lf
*.yaml text eol=lf

# Documentação
*.md text eol=lf
*.txt text eol=lf

# Código fonte Java
*.java text eol=lf

# Scripts
*.sh text eol=lf
*.bat text eol=crlf
*.cmd text eol=crlf

# Normalize todos os outros arquivos de texto
* text=auto
